tool int a(int a, int b, int... c):
    int r;
    for x in c:: r = r + x;
    return a + b + r;
end

tool int a(int a, int d):
    return a + d;
end

tool int a(int a, int e, int f):
    return a + e + f;
end

while true:: a(1, 1, c = 5);
