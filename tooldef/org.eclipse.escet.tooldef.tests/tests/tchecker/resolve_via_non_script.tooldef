// Various types of referable objects.

tool bool tool_(bool x):
    return x;
end

tool int tool_(int x):
    return x;
end

int var;

type type_ = bool;

import "../empty.txt" as imp;

// Not a 'via' imported script reference.
while true:: tool_.x();
while true:: var.x();
while true:: type_.x();
while true:: imp.x();
tool tool2<type_param>(int param):
    while true:: type_param.x();
    while true:: param.x();
end
