int x;
int y = x + 1;
int z = x + y * 2;
outln("%s %s %s", x, y, z);

tool int t(int x, int y, int z):
    return x + y + z;
end

outln("%s", t(1, 2, 3));
outln("%s", t(10, 20, 30));
