int z = 10;
while z > 0:
    outln("%s", z);
    z = z - 1;
end
outln();

z = 10;
while z > 0:
    if z == 5:: break;
    outln("%s", z);
    z = z - 1;
end
outln();

z = 10;
while z > 0:
    if z < 5:: break;
    z = z - 1;
    if z mod 2 == 1:: continue;
    outln("%s", z);
end
outln();

tool t():
    int x = 10;
    while true:
        if x < 5:: return;
        out("%s ", x);
        x = x - 1;
        outln("%s", x);
    end
end
t();
