type t = int;
t x = 5;
outln("%s", x);

x = x + 1;
outln("%s", x);

tool t f(t y):
    return y;
end

x = x + f(10);
outln("%s", x);

type t1 = t, t2 = t1;
t1 v1 = 3;
t2 v2 = 4;
outln("%s", v1 + v2);
