for x in range(20):
    if x mod 2 == 1:: continue;
    if x > 10:: break;
    outln("%d", x);
end
outln();

for x in range(10):
    for y in range(10):
        if y mod 2 == 1:: continue;
        if y > 3:: break;
        outln("%d %d", x, y);
    end
end
outln();
