@scanner Scanner;

@terminals:
  PLUS = "\+";
  MINUS = "\-";
  NAME = "a";
  @eof;
end

@main Sr1 : Parser;

@hooks Hooks;

{Class}
Sr1 : Sr1 PLUS Sr1
    | Sr1 MINUS Sr1
    | NAME
    ;
