model M():
    set int xr, yr;

    xr = {1,2,3,4};

    writeln("3 in %s = %s == true",  xr, 3 in xr);
    writeln("8 in %s = %s == false", xr, 8 in xr);
    writeln("<int>{} sub %s = %s == true", xr, <int>{} sub xr);
    writeln("{2,4} sub %s = %s == true", xr, {2,4} sub xr);
    writeln("{2,9} sub %s = %s == false", xr, {2,9} sub xr);

    xr = {1,2,3};
    write("%s\n", xr);
    xr = xr + {4};
    write("%s\n", xr);
    xr = xr - {4,5};
    write("%s\n", xr);
    xr = xr * {1,2};
    write("%s\n", xr);
end

