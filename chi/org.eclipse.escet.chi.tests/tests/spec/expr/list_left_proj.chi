model M():
  list(2) int xs;
  list list int ys;

  write("%s %s\n", xs, ys);
  xs = [1,2,3,4,5,6];
  write("%s %s\n", xs, ys);
  xs[1] = 14;
  write("%s %s\n", xs, ys);

  ys = [[1,2],[1]];
  write("%s %s\n", xs, ys);
  ys[0] = [1,2,3,4];
  write("%s %s\n", xs, ys);
  ys[1] = xs;
  write("%s %s\n", xs, ys);
  ys[1][2] = 5;
  write("%s %s\n", xs, ys);
end
