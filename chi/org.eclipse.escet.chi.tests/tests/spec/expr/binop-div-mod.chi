model M():
    int i = 7, j = 4;
    int d2, m2;

    d2 = i div j;
    m2 = i mod j;
    writeln("%s div %s = %s", i, j, d2);
    writeln("%s mod %s = %s", i, j, m2);

    j = -4;
    d2 = i div j;
    m2 = i mod j;
    writeln("%s div %s = %s", i, j, d2);
    writeln("%s mod %s = %s", i, j, m2);

    j = 4; i = -7;
    d2 = i div j;
    m2 = i mod j;
    writeln("%s div %s = %s", i, j, d2);
    writeln("%s mod %s = %s", i, j, m2);

    j = -4;
    d2 = i div j;
    m2 = i mod j;
    writeln("%s div %s = %s", i, j, d2);
    writeln("%s mod %s = %s", i, j, m2);
end
