model M():
    list int xs, ys;

    xs = [1,2,3,4];
    ys = del(xs, 2);
    writeln("%s == [1,2,4]\n", ys);

    writeln("%s contains 5: %s == false\n", xs, 5 in xs);
    writeln("%s contains 1: %s == true\n",  xs, 1 in xs);

    ys = xs[:] + xs[::] + xs[1:] + xs[:-1] + xs[::2] + xs[-1:0:-1];
end
