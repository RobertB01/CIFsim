func string convert(string s):
    string r, c;
    for k in range(size(s)):
        c = s[k];
        if c == ".": c = ","; end
        r = r + c
    end
    return r[:];
end

model M():
    string s = "abc.chi";
    string t;

    t = convert("abc.chi");
    writeln("\"%s\"", (s, t, s<t, s<=t, s>t, s>=t, s==t, s!=t));
end
