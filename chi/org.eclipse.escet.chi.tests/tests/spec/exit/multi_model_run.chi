model real M(int a):
    dist real d = uniform(1.0, 2.0);
    real r = sample d;

    write("%s + %s", a, r);
    exit a + r
end

xper X():
    real r;

    r = M(1);
    writeln("%s", r);

    r = M(2);
    writeln("%s", r);
end
