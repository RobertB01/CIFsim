model M():
    int a=1;
    tuple (int x; int y) t=(5, 8);

    write("%s %s\n", a, t);
    t.x = a;
    write("%s %s\n", a, t);
    a = t.y;
    write("%s %s\n", a, t);
    t.x = t.x + 1;
    write("%s %s\n", a, t);
end
