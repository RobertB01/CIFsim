type gst = tuple(int gid; real eq; list int dst);
type pd = tuple(int pid; list gst trv);
type tpd = tuple(pd x; timer t);

model Station2():
    chan? gst d;
    list tpd xst;
    int i;

    d?xst[i].x.trv[0];
end
