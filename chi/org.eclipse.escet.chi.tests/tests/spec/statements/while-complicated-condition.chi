model P():
    int count = 0;
    list int xs = [1,0];
    while xs[1:][0] < 5:
        xs = [size(xs)] + xs;

        count = count + 1;
        if count > 10:
            write("BOEM\n");
            break
        end
    end
    write("Finished\n");
end
