model m():
    run p1();
    run p2();
    run p3();
    run p4();
end

proc p1():
    for x in range(5):
        write("%d", x);
    end
end

proc p2():
    for x in range(5):
        write("%d", x);
    end;
end

proc p3():
    for x in range(5):
        write("%d", x);
    end
    for x in range(5):
        write("%d", x);
    end
end

proc p4():
    for x in range(5):
        write("%d", x);
    end;
    for x in range(5):
        write("%d", x);
    end
end
