type tt = tuple(int a; string b);

model M():
    tt t = (1, "abc");
    tuple(tt x; int a; real c) u;
    list(1) list(3) tt xs;

    writeln("%s %s %s", t, u, xs);
    xs[0][2] = t;
    writeln("%s %s %s", t, u, xs);
    t.a = 5;
    writeln("%s %s %s", t, u, xs);
    t.a = 4;
    writeln("%s %s %s", t, u, xs);

    u.a = 8;
    writeln("%s %s %s", t, u, xs);
    u.x.b = "blipblip";
    writeln("%s %s %s", t, u, xs);
end
