type t = tuple(int a; int b);

proc P(chan t a):
    t tt;
    int p,q;

    a?tt;
    writeln("%s", tt);
    p, q = tt;
    a!q, p;
end

proc Q(chan t a):
    t tt;
    int p,q;

    tt = 10,1;
    a!tt;
    a?p,q;
    writeln("%d, %d", p, q);
end

model M():
    chan t a;
    run P(a), Q(a)
end
