model M():
    int p=1, q=2, a=3, b=4;
    tuple (int x; int y) t;

    write("[%d, %d] <%d, %d> %s\n", p, q, a, b, t);
    p = q;
    write("[%d, %d] <%d, %d> %s\n", p, q, a, b, t);
    p,q = a,b;
    write("[%d, %d] <%d, %d> %s\n", p, q, a, b, t);

    a,b = b,a;
    write("[%d, %d] <%d, %d> %s\n", p, q, a, b, t);

    t = b,q;
    write("[%d, %d] <%d, %d> %s\n", p, q, a, b, t);
    a,b = t;
    write("[%d, %d] <%d, %d> %s\n", p, q, a, b, t);
end
