proc P():
  delay 5
end

model M():
  inst x;

  start x = P();

  writeln("finished %b", finished(x));

  delay 1;
  writeln("finished %b", finished(x));

  finish x;
  writeln("finished %b at time=%s (should be 5)", finished(x), time);

  delay 6;
  writeln("finished %b", finished(x));
end