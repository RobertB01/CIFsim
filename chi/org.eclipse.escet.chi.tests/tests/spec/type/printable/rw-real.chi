model void M():
    real v;

    writeln("%f#%6f#%-6f%%%6.2f", v, v, v, v);
    writeln("%g#%6g#%-6g%%%6.2g", v, v, v, v);
    writeln("%G#%6G#%-6G%%%6.2G", v, v, v, v);
    writeln("%e#%6e#%-6e%%%6.2e", v, v, v, v);
    writeln("%E#%6E#%-6E%%%6.2E", v, v, v, v);
    writeln("%s#%6s", v, v);
    exit;
    v = read(real);
end
