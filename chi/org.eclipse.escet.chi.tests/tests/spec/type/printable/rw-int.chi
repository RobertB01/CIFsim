model void M():
    int v = 30;

    writeln("%+4d#%+4d", v, -v);
    writeln("%4d#%04d#%-4d#", v, v, v);
    writeln("%4x#%04X#%-4x#", v, v, v);
    writeln("%s#%6s", v, v);
    exit;
    v = read(int);
end
