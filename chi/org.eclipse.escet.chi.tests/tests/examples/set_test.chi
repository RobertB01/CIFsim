model M():
    set int xr;

    xr = {1,2,3};
    write("%s\n", xr);
    xr = xr + {4};
    write("%s\n", xr);
    xr = xr - {4,5};
    write("%s\n", xr);
    xr = xr * {1,2};
    write("%s\n", xr);
end
