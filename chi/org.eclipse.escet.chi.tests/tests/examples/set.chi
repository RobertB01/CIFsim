model M():
   set int xr = {0,1,2,3}, yr; int N, x;
   N = size(xr);
   for i in range(N):
      xr = xr - {i};
      write("%d\t", i)
   end;
   xr = {0,1,2,3,4}; N = size(xr);
   for i in range(N):
      pass;
      x, yr = pop(xr);
      write("%d\t", x)
   end
end
