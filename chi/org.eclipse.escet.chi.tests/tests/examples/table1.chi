model M():
    run P(6)
end

func int f(int num):
    int j, i = 1;
    while i <= num:
        j = j + i * 8;
        i = i + 1
    end
    return j
end

proc P(int num):
    func int(int) ff = f;
    int y = ff(12);
    int x = f(num);
    write("%-20s %+d\n", x + sin(1.0), y)
end
