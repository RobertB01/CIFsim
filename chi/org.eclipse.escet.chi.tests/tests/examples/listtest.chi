model M():
   list int xs = [1,2,3], ys, zs = [0,1,2,3,4];
   list(2) list(5) int ms;
   write("%s\n", xs);
   ys = xs[1:];
   write("%s\n", ys);
   xs = xs[:2];
   write("%s\n", xs);
   write("%s\n", xs + ys);
   write("%s\n", zs);
   write("%s\n", ms);
   ms[0][1] = 5;
   write("%s\n", ms);
end
