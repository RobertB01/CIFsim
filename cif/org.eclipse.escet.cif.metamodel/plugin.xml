<plugin>

   <extension point="org.eclipse.emf.ecore.generated_package">
      <package
            uri="http://eclipse.org/escet/cif"
            class="org.eclipse.escet.cif.metamodel.cif.CifPackage"
            genModel="model/cif.genmodel"/>
   </extension>

   <extension point="org.eclipse.emf.ecore.generated_package">
      <package
            uri="http://eclipse.org/escet/cif/expressions"
            class="org.eclipse.escet.cif.metamodel.cif.expressions.ExpressionsPackage"
            genModel="model/cif.genmodel"/>
   </extension>

   <extension point="org.eclipse.emf.ecore.generated_package">
      <package
            uri="http://eclipse.org/escet/cif/functions"
            class="org.eclipse.escet.cif.metamodel.cif.functions.FunctionsPackage"
            genModel="model/cif.genmodel"/>
   </extension>

   <extension point="org.eclipse.emf.ecore.generated_package">
      <package
            uri="http://eclipse.org/escet/cif/cifsvg"
            class="org.eclipse.escet.cif.metamodel.cif.cifsvg.CifsvgPackage"
            genModel="model/cif.genmodel"/>
   </extension>

   <extension point="org.eclipse.emf.ecore.generated_package">
      <package
            uri="http://eclipse.org/escet/cif/print"
            class="org.eclipse.escet.cif.metamodel.cif.print.PrintPackage"
            genModel="model/cif.genmodel"/>
   </extension>

   <extension point="org.eclipse.emf.ecore.generated_package">
      <package
            uri="http://eclipse.org/escet/cif/types"
            class="org.eclipse.escet.cif.metamodel.cif.types.TypesPackage"
            genModel="model/cif.genmodel"/>
   </extension>

   <extension point="org.eclipse.emf.ecore.generated_package">
      <package
            uri="http://eclipse.org/escet/cif/automata"
            class="org.eclipse.escet.cif.metamodel.cif.automata.AutomataPackage"
            genModel="model/cif.genmodel"/>
   </extension>

   <extension point="org.eclipse.emf.ecore.generated_package">
      <package
            uri="http://eclipse.org/escet/cif/declarations"
            class="org.eclipse.escet.cif.metamodel.cif.declarations.DeclarationsPackage"
            genModel="model/cif.genmodel"/>
   </extension>

   <!-- Extension parser added to force xmi:id in serialization. -->
   <extension point="org.eclipse.emf.ecore.extension_parser">
      <parser
            type="cifx"
            class="org.eclipse.escet.common.emf.ecore.xmi.RealXMIResourceFactory">
      </parser>
   </extension>

</plugin>
