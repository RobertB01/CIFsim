PROGRAM MAIN
VAR
    timer_t: REAL;
    preset_timer_t: TIME;
    timer: DINT;
    sup: DINT;
    hw_button_bit: BOOL;
    hw_button: DINT;
    hw_lamp_bit: BOOL;
    hw_lamp: DINT;
    firstRun: BOOL := TRUE;
END_VAR
VAR_TEMP
    curValue: REAL;
    current_hw_button: DINT;
    current_hw_button__1: DINT;
    current_hw_lamp: DINT;
    current_hw_lamp__1: DINT;
    current_hw_lamp_bit: BOOL;
    current_hw_lamp_bit__1: BOOL;
    current_sup: DINT;
    current_sup__1: DINT;
    current_sup__2: DINT;
    current_sup__3: DINT;
    current_sup__4: DINT;
    current_sup__5: DINT;
    current_timer: DINT;
    current_timer__1: DINT;
    current_timer_t: REAL;
    edge_hw_button__1: DINT;
    edge_hw_lamp__1: DINT;
    edge_sup__1: DINT;
    edge_timer__1: DINT;
    eventEnabled: BOOL;
    isProgress: BOOL;
    timeOut: BOOL;
END_VAR

(* --- Read input from sensors. -------------------------------------------- *)
hw_button_bit := in_hw_button_bit;

(* --- Initialize state or update continuous variables. -------------------- *)
IF firstRun THEN
    firstRun := FALSE;

    (* Initialize the state variables. *)
    timer_t := 0.0;
    (* Reset timer of "timer_t". *)
    preset_timer_t := timer_t;
    ton_timer_t(IN := FALSE, PT := preset_timer_t);
    ton_timer_t(IN := TRUE, PT := preset_timer_t);
    timer := 0;
    sup := 0;
    hw_button := 0;
    hw_lamp_bit := FALSE;
    hw_lamp := 0;
ELSE
    (* Update remaining time of "timer_t". *)
    ton_timer_t(IN := TRUE, PT := preset_timer_t, Q => timeOut, ET => curValue);
    timer_t := SEL(timeOut, preset_timer_t - curValue, 0.0);
END_IF;

(* --- Process all events. ------------------------------------------------- *)
isProgress := TRUE;
(* Perform events until none can be done anymore. *)
WHILE isProgress DO
    isProgress := FALSE;

    (* Try to perform event "timer.start". *)
    eventEnabled := TRUE;
    IF timer = 0 THEN
        edge_timer__1 := 1;
    ELSE
        eventEnabled := FALSE;
    END_IF;
    IF eventEnabled THEN
        IF sup = 2 THEN
            edge_sup__1 := 1;
        ELSIF sup = 4 THEN
            edge_sup__1 := 2;
        ELSE
            eventEnabled := FALSE;
        END_IF;
    END_IF;
    IF eventEnabled THEN
        isProgress := TRUE;
        current_sup := sup;
        current_timer := timer;
        current_timer_t := timer_t;
        IF edge_timer__1 = 1 THEN
            timer_t := 3.0;
            (* Reset timer of "timer_t". *)
            preset_timer_t := timer_t;
            ton_timer_t(IN := FALSE, PT := preset_timer_t);
            ton_timer_t(IN := TRUE, PT := preset_timer_t);
            timer := 1;
        END_IF;
        IF edge_sup__1 = 1 THEN
            sup := 5;
        ELSIF edge_sup__1 = 2 THEN
            sup := 6;
        END_IF;
    END_IF;

    (* Try to perform event "timer.timeout". *)
    eventEnabled := TRUE;
    IF timer = 1 AND timer_t <= 0.0 THEN
        edge_timer__1 := 1;
    ELSE
        eventEnabled := FALSE;
    END_IF;
    IF eventEnabled THEN
        IF sup = 5 THEN
            edge_sup__1 := 1;
        ELSIF sup = 6 THEN
            edge_sup__1 := 2;
        ELSE
            eventEnabled := FALSE;
        END_IF;
    END_IF;
    IF eventEnabled THEN
        isProgress := TRUE;
        current_sup__1 := sup;
        current_timer__1 := timer;
        IF edge_timer__1 = 1 THEN
            timer := 0;
        END_IF;
        IF edge_sup__1 = 1 THEN
            sup := 7;
        ELSIF edge_sup__1 = 2 THEN
            sup := 8;
        END_IF;
    END_IF;

    (* Try to perform event "button.push". *)
    eventEnabled := TRUE;
    IF sup = 0 THEN
        edge_sup__1 := 1;
    ELSIF sup = 3 THEN
        edge_sup__1 := 2;
    ELSIF sup = 4 THEN
        edge_sup__1 := 3;
    ELSIF sup = 6 THEN
        edge_sup__1 := 4;
    ELSIF sup = 8 THEN
        edge_sup__1 := 5;
    ELSE
        eventEnabled := FALSE;
    END_IF;
    IF eventEnabled THEN
        IF hw_button = 0 AND hw_button_bit THEN
            edge_hw_button__1 := 1;
        ELSE
            eventEnabled := FALSE;
        END_IF;
    END_IF;
    IF eventEnabled THEN
        isProgress := TRUE;
        current_hw_button := hw_button;
        current_sup__2 := sup;
        IF edge_sup__1 = 1 THEN
            sup := 1;
        ELSIF edge_sup__1 = 2 THEN
            sup := 1;
        ELSIF edge_sup__1 = 3 THEN
            sup := 2;
        ELSIF edge_sup__1 = 4 THEN
            sup := 5;
        ELSIF edge_sup__1 = 5 THEN
            sup := 7;
        END_IF;
        IF edge_hw_button__1 = 1 THEN
            hw_button := 1;
        END_IF;
    END_IF;

    (* Try to perform event "button.release". *)
    eventEnabled := TRUE;
    IF sup = 1 THEN
        edge_sup__1 := 1;
    ELSIF sup = 2 THEN
        edge_sup__1 := 2;
    ELSIF sup = 5 THEN
        edge_sup__1 := 3;
    ELSIF sup = 7 THEN
        edge_sup__1 := 4;
    ELSIF sup = 9 THEN
        edge_sup__1 := 5;
    ELSE
        eventEnabled := FALSE;
    END_IF;
    IF eventEnabled THEN
        IF hw_button = 1 AND NOT hw_button_bit THEN
            edge_hw_button__1 := 1;
        ELSE
            eventEnabled := FALSE;
        END_IF;
    END_IF;
    IF eventEnabled THEN
        isProgress := TRUE;
        current_hw_button__1 := hw_button;
        current_sup__3 := sup;
        IF edge_sup__1 = 1 THEN
            sup := 3;
        ELSIF edge_sup__1 = 2 THEN
            sup := 4;
        ELSIF edge_sup__1 = 3 THEN
            sup := 6;
        ELSIF edge_sup__1 = 4 THEN
            sup := 8;
        ELSIF edge_sup__1 = 5 THEN
            sup := 0;
        END_IF;
        IF edge_hw_button__1 = 1 THEN
            hw_button := 0;
        END_IF;
    END_IF;

    (* Try to perform event "lamp.on". *)
    eventEnabled := TRUE;
    IF sup = 1 THEN
        edge_sup__1 := 1;
    ELSIF sup = 3 THEN
        edge_sup__1 := 2;
    ELSE
        eventEnabled := FALSE;
    END_IF;
    IF eventEnabled THEN
        IF hw_lamp = 0 THEN
            edge_hw_lamp__1 := 1;
        ELSE
            eventEnabled := FALSE;
        END_IF;
    END_IF;
    IF eventEnabled THEN
        isProgress := TRUE;
        current_hw_lamp := hw_lamp;
        current_hw_lamp_bit := hw_lamp_bit;
        current_sup__4 := sup;
        IF edge_sup__1 = 1 THEN
            sup := 2;
        ELSIF edge_sup__1 = 2 THEN
            sup := 4;
        END_IF;
        IF edge_hw_lamp__1 = 1 THEN
            hw_lamp_bit := TRUE;
            hw_lamp := 1;
        END_IF;
    END_IF;

    (* Try to perform event "lamp.off". *)
    eventEnabled := TRUE;
    IF sup = 7 THEN
        edge_sup__1 := 1;
    ELSIF sup = 8 THEN
        edge_sup__1 := 2;
    ELSE
        eventEnabled := FALSE;
    END_IF;
    IF eventEnabled THEN
        IF hw_lamp = 1 THEN
            edge_hw_lamp__1 := 1;
        ELSE
            eventEnabled := FALSE;
        END_IF;
    END_IF;
    IF eventEnabled THEN
        isProgress := TRUE;
        current_hw_lamp__1 := hw_lamp;
        current_hw_lamp_bit__1 := hw_lamp_bit;
        current_sup__5 := sup;
        IF edge_sup__1 = 1 THEN
            sup := 9;
        ELSIF edge_sup__1 = 2 THEN
            sup := 0;
        END_IF;
        IF edge_hw_lamp__1 = 1 THEN
            hw_lamp_bit := FALSE;
            hw_lamp := 0;
        END_IF;
    END_IF;
END_WHILE;

(* --- Write output to actuators. ------------------------------------------ *)
out_hw_lamp_bit := hw_lamp_bit;
END_PROGRAM
