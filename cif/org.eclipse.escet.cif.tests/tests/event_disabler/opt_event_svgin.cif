svgfile "../pprint_elim_cdef/text.svg";

event e, f;
svgin id "txt1" event e;

group g:
  svgin id "txt2" event e;

  event $plant, $automaton, $requirement;

  automaton a:
    location:
      initial;
      edge f;

    svgin id "svg2" event if true: $plant elif true: $automaton else $requirement end;
  end
end
