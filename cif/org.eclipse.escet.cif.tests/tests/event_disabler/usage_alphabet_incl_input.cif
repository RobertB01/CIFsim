// i = in alphabet, n = not in alphabet, s = supplied, u = unsupplied
event is, iu, ns, nu;

group g:
  event is, iu, ns, nu;

  automaton a:
    event is, iu, ns, nu;

    location:
      initial;
      edge ^is, ^iu, ^g.is, ^g.iu, ^g.a.is, ^g.a.iu;
  end
end
