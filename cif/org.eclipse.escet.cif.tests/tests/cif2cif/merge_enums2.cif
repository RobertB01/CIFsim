group g1:
  enum e1 = A, B;
end
group g2:
  enum e2 = B, A;
end
plant p:
  disc g1.e1 v1;
  disc g2.e2 v2;
  initial v1 = g1.A or v2 = g2.A or v1 = g1.B or v2 = g2.B;
  location:
    initial;
end
