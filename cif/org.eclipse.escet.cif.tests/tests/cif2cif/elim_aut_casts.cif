automaton a1_noname:
  alg string v1 = <string>self;
  alg string v2 = <string>a1_noname;
  alg string v3 = <string>true;

  location:
    initial;
end

automaton a1_named:
  alg string v1 = <string>self;
  alg string v2 = <string>a1_named;
  alg string v3 = <string>true;

  location l:
    initial;
end

automaton a2:
  alg string v1 = <string>self;
  alg string v2 = <string>a2;
  alg string v3 = <string>true;

  location l1:
    initial;
  location l2:
    initial;
end

automaton a3:
  alg string v1 = <string>self;
  alg string v2 = <string>a3;
  alg string v3 = <string>true;

  location l1:
    initial;
  location l2:
    initial;
  location l3:
    initial;
end

automaton def B():
  location:
    initial;
end

b1: B();
b2: B();

alg string x1 = <string>b1;
alg string x2 = <string>b2;
