input bool ivar;
alg bool v1 = not ivar; // Used.

alg bool v2; // Used.
equation v2 = v1;
alg bool v3 = v2; // Used.

alg bool v4;
equation v4 = v1;
alg bool v5 = v4;

plant A:
  uncontrollable a;
  location:
    initial; marked;
    edge a when v3;
end

alg bool v6 = false; // Used.
requirement A.a needs v6;

alg bool v7 = false; // Used.

plant B:
  controllable b;
  disc bool cond = false;
  location:
    initial; marked;
    edge b do cond := v7;
end
