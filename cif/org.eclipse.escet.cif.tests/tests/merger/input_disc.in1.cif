type t1 = real;
type t3 = t1;

group a:
  input real inp1;
  input t1 inp2;
  input t3 inp3;

  plant invariant inp1 + inp2 + inp3 > 0;
end

group annos:
  @cif:typechecker:tests:anno2
  @cif:typechecker:tests:anno4
  input bool x;
  invariant x;
end
