const real t2 = 8.9;

cont inp1 = 1.23 der t2;
invariant inp1 > 1;

cont inp2;
equation inp2' = 2.34;
invariant inp2 > 1;

group g:
  cont inp3;
  invariant inp3 > 1;
  equation inp3' = 2.34;
end

plant p:
  cont inp4;
  plant invariant inp4 > 1;
  location:
    initial;
    equation inp4' = 2.34;
end

group q:
  cont inp5;
  equation inp5' = 3.45;
  plant invariant inp5 > 0;
end
