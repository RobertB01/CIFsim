type t1 = real;

input t1 inp1;
invariant inp1 > 0;

input t1 inp2;
invariant inp2 > 0;

group g:
  input t1 inp3;
  invariant inp3 > 0;
end

group p:
  input t1 inp4;
  plant invariant inp4 > 0;
end

plant q:
  input t1 inp5;
  plant invariant inp5 > 1;
  location:
    initial;
end
