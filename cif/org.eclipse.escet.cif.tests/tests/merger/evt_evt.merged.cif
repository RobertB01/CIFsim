event int e1;
event e2;
event void e3;
plant automaton p1:
  location:
    initial;
    edge e1!1;
    edge e2;
    edge e3?;
end
plant automaton q1:
  disc int x;
  location:
    initial;
    edge e1? do x := ?;
end
group annos:
  @cif:typechecker:tests:anno1(arg = true)
  @cif:typechecker:tests:anno3
  @cif:typechecker:tests:anno2
  @cif:typechecker:tests:anno4
  event e;
  invariant e needs true;
  invariant e needs true;
end
plant automaton p2:
  location:
    initial;
    edge e1?;
    edge e2;
    edge e3;
end
plant automaton q2:
  location:
    initial;
    edge e3!;
end
