type t2 = real;

plant a:
  disc t2 inp1;
  disc t2 inp2 = 1.23;
  disc t2 inp3 in {2.3, 3.4};
  plant invariant inp1 + inp2 + inp3 > 1;

  location:
    initial;
end

automaton annos:
  @cif:typechecker:tests:anno1(arg = true)
  @cif:typechecker:tests:anno3
  disc bool x;
  invariant x;

  location:
    initial;
end
