automaton p:
  disc string x;
  location:
    initial;
    edge do x[1] := "a";
end

func bool f():
  string x;
  x[1] := "a";
  return true;
end

invariant f();

automaton q:
  disc list string x;
  location:
    initial;
    edge do x[0][1][2][3] := "a";
end

input string i;
svgin id "" do i[1] := "a";
