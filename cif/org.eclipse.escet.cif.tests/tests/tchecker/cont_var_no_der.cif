// No derivative.
cont x;
invariant x > 0;

// Incomplete derivatives.
automaton p:
  cont x;
  invariant x > 0;

  location l1:
    initial;
    equation x' = 1.0;

  location l2:
    initial;

  location l3:
    initial;
    equation x' = 1.0;

  location l4:
    initial;
    equation x' = 1.0;

  location l5:
    initial;
end
