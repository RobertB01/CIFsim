func bool f1():
  int x;
  x := 5;
  return true;
  x := 6;
  return true;
end

func bool f2():
  int x;
  x := 5;
  while true:
    x := x + 1;
    break;
    x := 6;
    x := 7;
  end
  while true:
    x := x + 1;
    continue;
    x := 6;
    x := 7;
  end
  return true;
end

func bool f3():
  int x;
  x := 5;
  if x > 0:
    return true;
  else
    return false;
  end
  x := 6;
  return true;
end

func bool f4():
  int x;
  x := 5;
  if x > 0:
    return true;
  elif true:
    return true;
  else
    return false;
  end
  x := 6;
  return true;
end

func bool f5():
  int x;
  return true;
  while true:
    x := 6;
  end
  return false;
end

invariant f1() or f2() or f3() or f4() or f5();
