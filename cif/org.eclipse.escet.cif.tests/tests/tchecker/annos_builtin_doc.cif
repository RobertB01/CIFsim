// Missing argument.
@doc
input bool i1;

// Unsupported arguments.
@doc(a = "a", $text = "text", b = "b")
input bool i2;

// Not a string-typed argument.
@doc($text = false)
input bool i3;

// Not a statically-evaluable argument.
@doc($text = fmt("%s", i4))
input bool i4;

// Argument evaluation error.
@doc($text = fmt("%s", sqrt(-1) > 0))
input bool i5;
