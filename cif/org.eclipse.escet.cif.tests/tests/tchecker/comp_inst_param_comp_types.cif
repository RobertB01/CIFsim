group g:
  group p:
    invariant true;
  end

  plant def pdef():
    location l:
      initial;
  end

  pinst: pdef();

  group def X(pdef p2):
    invariant p2.l;
  end

  x_p:     X(p);
  x_pinst: X(pinst);
  x_bool:  X(true);
end
