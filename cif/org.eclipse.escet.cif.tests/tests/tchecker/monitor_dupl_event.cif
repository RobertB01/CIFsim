group g:
  plant x:
    event a, b;
    monitor a, a, b;
    location:
      initial;
      edge a, b;
  end
end

///

group def P():
  event e;
end

plant def Q(P p1, p2):
  // May both be false positive, depending on instantiations.
  monitor p1.e, p2.e;
  location:
    initial;
    // May both be false positive, depending on instantiations.
    edge p1.e, p2.e;
end
