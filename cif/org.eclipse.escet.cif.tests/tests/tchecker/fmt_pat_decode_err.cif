// Duplicate flag.
print fmt("%--s", 5);
print fmt("%++s", 5);
print fmt("%  s", 5);
print fmt("%00s", 5);
print fmt("%,,s", 5);
print fmt("%-+-+s", 5);

// Percentage with other stuff.
print fmt("%1$%", 5);
print fmt("%+%", 5);
print fmt("%5%", 5);
print fmt("%.3%", 5);

// Missing index/precision.
print fmt("%$s", 5);
print fmt("%$", 5);
print fmt("%.s", 5);
print fmt("%.", 5);

// Invalid flag combinations.
print fmt("%+ s", 5);
print fmt("% +,s", 5);
print fmt("%-0s", 5);
print fmt("%0-,s", 5);

// Missing width.
print fmt("%-s", 5);
print fmt("%0s", 5);

// Unknown formatting conversion.
print fmt("%z", 5);
print fmt("%1$z", 5);
print fmt("%+z", 5);
print fmt("%2z", 5);
print fmt("%.3z", 5);
print fmt("%1$+2.3z", 5);

// Incomplete specifier.
print fmt("%", 5);
print fmt("%1$", 5);
print fmt("%+", 5);
print fmt("%2", 5);
print fmt("%.3", 5);

// Test offset/length, with escaping.
print fmt("a\tb\\c%sd%%e%sf", 1);

// All conversion with flag/width/precision combinations.
