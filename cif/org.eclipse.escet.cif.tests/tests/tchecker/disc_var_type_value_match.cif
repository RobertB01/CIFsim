automaton p:
  disc bool x1 = 5;
  disc bool x2 in {true, 1, 2};
  disc int[2..2] x3 = 2;
  disc int[2..3] x4 = 2;
  disc int[1..2] x5 in {0, 1, 2, 3, x3, x4};
  disc list[0..3] int x6 = [1, 2, 3, 4];
  disc list[3] int x7 = [1, 2, 3, 4];

  invariant x1 or x2 or x3 = 2 or x4 = 2 or x5 = 2;

  location:
    initial;
end
