func list int f(int x):
  // Note that the local variables are ordered on their dependencies to other
  // local variables, for their initial values.
  int g, h = 1;
  int x3 = x2 + 1;
  int x2 = x + 1;

  return [g, h, x, x2, x3];
end

automaton p:
  disc list int x = f(5);

  invariant x = [0, 1, 5, 6, 7];

  location:
    initial;
    urgent;
end
