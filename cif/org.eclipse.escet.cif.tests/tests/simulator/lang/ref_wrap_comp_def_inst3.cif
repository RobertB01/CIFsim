// Test for bug 'Wrapping expressions need wrapping types', with 3 levels of component instantiations.
enum E1 = X, Y;

group def G():
  automaton aa:
    enum E2 = Q, R;

    disc bool v;

    disc E1 v1 = X;
    disc E2 v2 = Q;

    disc list E1 w1 = [Y];
    disc list E2 w2 = [R];

    location l:
      initial;
      urgent;
  end
end

group def H():
  g: G();
end

group def I():
  h: H();
end

i1: I();

print i1.h.g.aa.v;
print i1.h.g.aa.v1;
print i1.h.g.aa.v2;
print i1.h.g.aa.w1;
print i1.h.g.aa.w2;
