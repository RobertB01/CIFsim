// Test for bug 'Wrapping expressions need wrapping types', with component parameters.
enum E1 = X, Y;

group def G():
  automaton aa:
    enum E2 = Q, R;

    disc bool v;

    disc E1 v1 = X;
    disc E2 v2 = Q;

    disc list E1 w1 = [Y];
    disc list E2 w2 = [R];

    location l:
      initial;
      urgent;
  end
end

g1: G();

group def Z(G g):
  print g.aa.v;
  print g.aa.v1;
  print g.aa.v2;
  print g.aa.w1;
  print g.aa.w2;
end

z1: Z(g1);
