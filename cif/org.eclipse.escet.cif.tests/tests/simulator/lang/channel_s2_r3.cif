automaton def S(event int e; alg int v):
  location l1:
    initial;
    edge e!v goto l2;

  location l2:
    urgent;
end

automaton def R(event int e):
  disc int v;

  location l1:
    initial;
    edge e? do v := ? goto l2;

  location l2:
    urgent;
end

event int e;

s1: S(e, 1);
s2: S(e, 2);

r1: R(e);
r2: R(e);
r3: R(e);
