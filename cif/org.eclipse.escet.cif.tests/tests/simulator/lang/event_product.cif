// 'Normal' automata.
event e;

automaton p:
  location l1:
    initial;
    edge e goto l2;
    edge e;

  location l2:
    edge e goto l3;

  location l3:
    urgent;
end

automaton q:
  location:
    initial;
    edge e;
    edge e;
    edge e;
end

// 'Simple' automata.
event f;

automaton r:
  location l1:
    initial;
    edge f goto l2;
    edge f;

  location l2:
    edge f goto l3;

  location l3:
    urgent;
end

automaton s:
  location:
    initial;
    edge f;
    edge f;
    edge f;
end
