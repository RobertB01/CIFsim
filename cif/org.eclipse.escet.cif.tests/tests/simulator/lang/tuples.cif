// Single generated code class for compatible tuples.

automaton p:
  disc tuple(int a, b) tii1 = (1, 2);
  disc tuple(int c, d) tii2 = (3, 4);

  location l1:
    initial;
    edge do (tii1, tii2) := (tii2, tii1) goto l2; // swap

  location l2:
    edge do tii1 := (tii1[a], tii2[c]) goto l3;   // tii1 = (3, 2)

  location l3:
    urgent;
end

