// expm1(x) = exp(x) - 1
func real f(real x): "java:java.lang.Math.expm1";

plant p:
  // Function called multiple times, and verified against reference
  // implementation.
  disc list real l = [f(2.0), f(2.0), exp(2.0) - 1];
  initial size(l) > 0;

  location:
    initial;
    urgent;
end
