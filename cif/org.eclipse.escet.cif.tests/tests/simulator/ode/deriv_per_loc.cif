automaton m:
  cont x = 0.0;

  location m1:
    initial;
    equation x' = 1.0;
    edge when x >= 2.0 goto m2;

  location m2:
    equation x' = 2.0;
    edge when x >= 4.0 goto m3;

  location m3:
    equation x' = 3.0;
end
