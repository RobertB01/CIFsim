svgfile "out_txt.svg";

svgcopy id "txt" pre "a";
svgcopy id "txt" post "b";
svgcopy id "txt" pre "c" post "d";

group def X(alg int nr):
  svgcopy id "txt" post "_" + <string>nr;

  svgout id "txt_" + <string>nr text value "abc" + <string>nr;
end

x1: X(1);
x2: X(2);
x3: X(3);
x4: X(4);
x5: X(5);

automaton a:
  location:
    initial;
    urgent;
end
