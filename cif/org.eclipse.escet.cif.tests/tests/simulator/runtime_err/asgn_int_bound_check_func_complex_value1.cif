func bool f():
  list tuple(int[0..2] a; dict(int[5..6]:set int[8..9]) b) x;
  int y = 10;

  x := [(0, {5: {}})];            // ok
  x := [(1, {6: {8}})];           // ok
  x := [(2, {6: {9}})];           // ok
  x := [(0, {5: {y}})];           // err (y > 9)
  return true;
end

invariant f();
