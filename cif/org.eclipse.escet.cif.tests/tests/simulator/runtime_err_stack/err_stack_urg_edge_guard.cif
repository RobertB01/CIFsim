// Guard for automaton "q" is not evaluated, since event "e" is not enabled
// in automaton "p". For the time transition however, the guard of the urgent
// edge is evaluated.

event e;

automaton p:
  location:
    initial;
    edge e when false;
end

automaton q:
  location:
    initial;
    edge e when 1 / 0 > 0 now;
end
