// Second guard predicate is not evaluated for the "tau" transition, since the
// first guard evaluates to "false". For the time transition however, timed
// guard "1 / 0 > time" is still used.

automaton p:
  cont x = 2.0 der -1.0;
  location:
    initial;
    edge when false, sqrt(x) > time;
end
