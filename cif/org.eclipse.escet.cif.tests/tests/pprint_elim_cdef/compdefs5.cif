automaton def P(Q qq):
  invariant qq.ql, pl;
  location pl:
    initial;
end

automaton def Q(P pp):
  invariant pp.pl, ql;
  location ql:
    initial;
end

p1: P(q1);
q1: Q(p1);

invariant p1.pl, q1.ql;
