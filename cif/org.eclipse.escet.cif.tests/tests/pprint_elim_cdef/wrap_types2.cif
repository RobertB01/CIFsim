group def Q():
  enum e = A, B;
  type te = e;
  type tb = bool;
  invariant A = B;
end

group def P():
  q1: Q();
  q2: Q();
end

p1: P();
p2: P();

automaton aut:
  disc p1.q1.e v11;
  disc p1.q2.e v12;
  disc p2.q1.e v21;
  disc p2.q2.e v22;
  disc p1.q1.te w11;
  disc p1.q2.te w12;
  disc p2.q1.te w21;
  disc p2.q2.te w22;
  disc p1.q1.tb x11;
  disc p1.q2.tb x12;
  disc p2.q1.tb x21;
  disc p2.q2.tb x22;
  invariant v11 = v12 and v12 = v21 and v21 = v22;
  invariant w11 = w12 and w12 = w21 and w21 = w22;
  invariant x11 = x12 and x12 = x21 and x21 = x22;
  location:
    initial;
end
