type tt = tuple(int a; bool b; string c; real d);
const bool a = true;
const string b = "b";
const int[0..0] c = 0;
automaton p:
  disc list int c;
  disc tt tv;
  const int[0..0] b = 0;
  alg int pa = tv[a];
  alg bool pb = tv[b];
  alg string pc = tv[c];
  alg real pd = tv[d];
  alg int p1 = tv[.p.b];
  alg int p2 = tv[.c];
  invariant pa > 0 or pb or pc = "" or pd = 0.0;
  location:
    initial;
end
invariant a or b = "" or size(p.c) > 0;
