group def A(B bb):
  e: E(bb.i.d);
end

group def B():
  i: I();
end

group def I():
  d: D();
end

automaton def D():
  type t = int[2..5];
  location m:
    initial;
end

group def E(D dd):
  invariant dd.m;
  alg dd.t x = 3;
end

b: B();

a: A(b);
