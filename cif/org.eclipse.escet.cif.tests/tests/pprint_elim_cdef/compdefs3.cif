automaton def P(location pp):
  invariant pp, pl;
  location pl:
    initial;
end

automaton def Q(location qp):
  invariant qp, ql;
  location ql:
    initial;
end

p1: P(q1.ql);
q1: Q(p1.pl);

invariant p1.pl, q1.ql;
