group def P():
  automaton def Q():
    invariant q1.l, l;
    location l:
      initial;
  end

  q1: Q();
  q2: Q();
end

p1: P();
p2: P();

invariant p1.q1.l, p1.q2.l, p2.q1.l, p2.q2.l;
