automaton a1:
  @state(a = 1, b = 2)
  @state(a = 2, b = 3)
  location:
    initial;
end

automaton a2:
  @state(a = 1, b = 2)
  location loc1:
    initial;

  @state(a.b.c = "a", d.e.f = [1, 2])
  location loc2:
    initial;
end
