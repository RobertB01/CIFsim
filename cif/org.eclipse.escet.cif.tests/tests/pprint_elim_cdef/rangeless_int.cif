type t = int;
type t2 = t;

const t c = 123;

alg t2 a = q.x + c;
invariant a > 123;

automaton q:
  disc t2 x;
  invariant x > 0;
  location:
    initial;
end

group def P(alg int ip):
  invariant pow(ip - 1, 312) * 5 > -239;
end
