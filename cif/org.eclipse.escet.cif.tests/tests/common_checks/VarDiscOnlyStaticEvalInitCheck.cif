automaton A:
  disc real v1 = sqrt(-1.0);
  disc real v2 in {sqrt(-1.0), sqrt(-1.0)};
  disc bool v3 = (v1 = v2);
  disc bool v4 in {(v1 = v2), (v1 = v2)};
  disc real v5 in any;
  location:
    initial;
end
