automaton A:
  controllable a, b, c, d, e, f, g, h, i, j, k, l, m, n, o;
  location l0:
    initial;
    edge a goto l1;
  location l1:
    edge b goto l2;
    edge c goto l4;
    edge d goto l5;
  location l2:
    edge e goto l3;
    edge f goto l6;
  location l3:
    edge g goto l2;
    edge h goto l7;
  location l4:
    edge i goto l0;
    edge j goto l5;
  location l5:
    edge k goto l6;
  location l6:
    edge l goto l5;
  location l7:
    edge m goto l3;
    edge n goto l6;
    edge o goto l7;
end
