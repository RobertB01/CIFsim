automaton A:
  controllable a, b;
  input bool x, y;
  location l0:
    initial;
    edge a when x goto l1;
    edge a when not x or y goto l2;
    edge b when x goto l1;
    edge b when not x goto l2;
   location l1;
   location l2;
end
