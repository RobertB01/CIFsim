@@controller:properties(boundedResponse: true, uncontrollablesBound: 1, controllablesBound: 0, confluence: true, finiteResponse: true)
input bool sensor;
uncontrollable u_on;
uncontrollable u_off;
plant automaton p:
  location Off:
    initial;
    marked;
    edge u_on when sensor goto On;
  location On:
    edge u_off when not sensor goto Off;
end
