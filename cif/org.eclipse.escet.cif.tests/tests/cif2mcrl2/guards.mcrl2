    ((a'x' == 1)) -> f' . P()
    +
    (((a'x' == 2) && (a'x' != 3))) -> g' . P()
    +
    (((a'y' == 5) && ((-2147483648 <= (a'x' + 1)) && ((a'x' + 1) <= 2147483647)))) -> h' . P(a'x' = (a'x' + 1))
    +
    % CIF variable value actions.
    a'x''varvalue(a'x') . P()
    +
    a'y''varvalue(a'y') . P()
    +
