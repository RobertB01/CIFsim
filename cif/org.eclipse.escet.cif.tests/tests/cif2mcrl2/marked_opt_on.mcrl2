proc P(
    m'v': Int
) =
    % CIF linearized edges.
    (((m'v' < 5) && (((-2147483647 - 1) <= (m'v' + 1)) && ((m'v' + 1) <= 2147483647)))) -> m'e' . P(m'v' = (m'v' + 1))
