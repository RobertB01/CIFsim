% Generated by CIF to mCRL2.

% Sorts for CIF enumerations.
sort A'LPE' = struct A'AS0' | A'AS1';

% Actions for CIF events.
act A'a';
act A'b';
act A'c';

% Actions for CIF variables having certain values.
act A''varvalue: A'LPE';
act A'x''varvalue: Int;

% Action for CIF marker predicate.
act marked;

% Process for behavior of the CIF specification.
proc P(
    A'x': Int,
    A': A'LPE'
) =
    % CIF linearized edges.
    ((A' == A'AS0')) -> A'a' . P(A'x' = 0, A' = A'AS1')
    +
    ((((A' == A'AS1') && (A'x' < 5)) && ((0 <= (A'x' + 1)) && ((A'x' + 1) <= 6)))) -> A'b' . P(A'x' = (A'x' + 1))
    +
    ((((A' == A'AS1') && (A'x' == 5)) && (A'x' == 5))) -> A'c' . P(A'x' = 0, A' = A'AS0')
    +
    % CIF variable value actions.
    A''varvalue(A') . P()
    +
    A'x''varvalue(A'x') . P()
    +
    % CIF 'marked' action.
    (false) -> marked . P()
;

% Initialization.
init P(0, A'AS0');
