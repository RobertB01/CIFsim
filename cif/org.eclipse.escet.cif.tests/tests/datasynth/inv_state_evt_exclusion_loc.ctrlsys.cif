controllable b;
controllable e;
controllable f;
controllable g;
controllable h;
plant automaton a:
  disc int[0..25] x = 1;
  location l1:
    initial;
    edge e when x < 20 do x := x + 1;
    edge f;
    edge g;
    edge h;
    edge b when x = 5 goto l2;
  location l2:
    marked;
    edge e when x < 20 do x := x + 1;
    edge f;
    edge g;
    edge h;
end
supervisor automaton sup:
  alphabet e, f, g, h, b;
  location:
    initial;
    marked;
    edge b when true;
    edge e when a.l1 and (a.x = 0 or a.x = 4) or (a.l1 and a.x = 2 or a.l1 and (a.x = 1 or a.x = 3)) or (a.l2 and (16 <= a.x and a.x <= 19) or (a.l2 and (8 <= a.x and a.x <= 11 or (a.x = 24 or a.x = 25)) or a.l2 and (4 <= a.x and a.x <= 7 or (12 <= a.x and a.x <= 15 or 20 <= a.x and a.x <= 23))));
    edge f when a.l2 and (a.x = 12 or a.x = 13) or a.l2 and (a.x = 10 or a.x = 11 or (a.x = 14 or a.x = 15));
    edge g when a.l2 and (a.x = 12 or a.x = 14) or (a.l2 and a.x = 13 or a.l2 and a.x = 11);
    edge h when a.l1 and a.x = 4 or a.l1 and a.x = 2 or (a.l1 and (a.x = 1 or a.x = 5) or (a.l1 and a.x = 3 or a.l2 and (a.x = 12 or a.x = 13)));
end
