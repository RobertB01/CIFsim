plant p:
  controllable c, $event;

  location:
    initial;
    marked;
    edge c;
    edge c; // Same event label, should only be listed once.
    edge $event;
end

plant q:
  controllable c;

  location:
    initial;
    marked;
    edge c;
end

input int[0..3] x;
input bool y;
