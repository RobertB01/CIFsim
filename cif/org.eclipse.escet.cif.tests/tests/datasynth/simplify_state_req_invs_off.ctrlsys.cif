controllable e3a;
plant automaton p3:
  disc int[0..5] x in any;
  location loc1:
    initial;
    marked;
    edge e3a goto loc2;
  location loc2:
    edge e3a goto loc1;
end
supervisor automaton sup:
  alphabet e3a;
  initial p3.loc1 and p3.x = 4 or (p3.loc1 and p3.x = 5 or p3.loc1 and p3.x = 3);
  location:
    initial;
    marked;
    edge e3a when p3.loc1 and (p3.x = 4 or p3.x = 5) or p3.loc2 and p3.x = 4 or (p3.loc2 and p3.x = 5 or p3.loc2 and p3.x = 3);
end
