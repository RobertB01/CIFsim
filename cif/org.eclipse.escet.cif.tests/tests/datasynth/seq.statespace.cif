controllable c1;
controllable c2;
uncontrollable u1;
uncontrollable u2;
automaton statespace:
  alphabet c1, u2, u1, c2;
  @state(p = "m0", p.x = 3, sup = "*")
  location loc1:
    initial;
    edge c1 goto loc2;
  @state(p = "m1", p.x = 3, sup = "*")
  location loc2:
    edge u2 goto loc3;
  @state(p = "m2", p.x = 4, sup = "*")
  location loc3:
    edge u1 goto loc4;
  @state(p = "m3", p.x = 5, sup = "*")
  location loc4:
    edge u1 goto loc5;
  @state(p = "m4", p.x = 6, sup = "*")
  location loc5:
    edge u1 goto loc6;
  @state(p = "m5", p.x = 7, sup = "*")
  location loc6:
    marked;
end
