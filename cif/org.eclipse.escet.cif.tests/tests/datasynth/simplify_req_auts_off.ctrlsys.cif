controllable e1a;
controllable e1b;
plant automaton p1:
  disc int[0..5] x in any;
  location:
    initial;
    marked;
    edge e1a when x >= 3;
    edge e1b when x >= 3;
end
supervisor automaton r1:
  location:
    initial;
    marked;
    edge e1a when p1.x >= 2;
    edge e1b when p1.x >= 4;
end
supervisor automaton sup:
  alphabet e1a, e1b;
  location:
    initial;
    marked;
    edge e1a when p1.x != 0 and (p1.x != 2 and p1.x != 1);
    edge e1b when p1.x = 4 or p1.x = 5;
end
