plant automaton p:
  controllable c1;
  controllable c2;
  controllable c3;
  disc int[0..4] x in any;
  disc int[0..4] y in any;
  disc int[0..3] a in any;
  disc int[1..7] b in any;
  disc int[3..6] c in any;
  location:
    initial y != 1;
    marked y != 3;
    edge c1 do x := y;
    edge c2 do a := b;
    edge c3 do a := c;
end
