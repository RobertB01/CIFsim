input bool D;
input bool C;
plant automaton p:
  input bool Y;
  input bool X;
  controllable c;
  controllable a;
  controllable v;
  location l1:
    initial;
    marked;
    edge c;
    edge a;
    edge v;
end
supervisor automaton sup:
  alphabet p.c, p.a, p.v;
  location:
    initial;
    marked;
    edge p.a when true;
    edge p.c when true;
    edge p.v when true;
end
