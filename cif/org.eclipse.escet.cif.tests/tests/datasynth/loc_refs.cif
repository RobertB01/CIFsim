controllable e1, e2, e3, e4;

plant pe:
  marked a;

  location a:
    initial;
    marked;

    edge e1 goto b;
    edge e2 goto c;
    edge e3;
    edge e4 goto a;

  location b:
    marked;

    edge e1 goto a;

  location c:
    marked;
end

controllable f1, f2;

plant pf:
  initial a;

  location a:
    initial;
    marked;

    edge f1;
    edge f2 goto a;
end

plant q1:
  disc int[0..5] x;
  disc int[1..3] y;

  initial x = 0, y = 1;

  location:
    initial;
    marked;
end
