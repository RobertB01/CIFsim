uncontrollable e;
automaton statespace:
  alphabet e;
  @state(p = "l1", p.x = 0, sup = "*")
  location loc1:
    initial;
    edge e goto loc3;
  @state(p = "l1", p.x = 1, sup = "*")
  location loc2:
    initial;
    edge e goto loc4;
    edge e goto loc5;
    edge e goto loc6;
  @state(p = "l2", p.x = 0, sup = "*")
  location loc3:
    marked;
  @state(p = "l2", p.x = 2, sup = "*")
  location loc4:
    marked;
  @state(p = "l2", p.x = 3, sup = "*")
  location loc5:
    marked;
  @state(p = "l2", p.x = 4, sup = "*")
  location loc6:
    marked;
end
