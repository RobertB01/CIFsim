controllable c1, c2;

plant p1:
  location l0:
    initial;
    marked;
    edge c1 goto l1;

  location l1:
    edge c2 goto l0;
end

plant p2:
  alphabet c1, c2;

  location l0:
    initial;
    marked;
    edge c1;
end
