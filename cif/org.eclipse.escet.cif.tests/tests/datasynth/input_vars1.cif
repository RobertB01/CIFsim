enum E = A, B;

group io:
  input bool b;
  input int[0..3] i;
  input E e;
end

plant p:
  location:
    initial;
    marked;
end

// Can't prevent violating this requirement, as these inputs are unrestricted
// and can thus change arbitrarily.
requirement io.b and io.i = 3 and io.e = A;
