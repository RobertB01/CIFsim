controllable e2a;
controllable e2b;
controllable e2c;
controllable e2d;
plant automaton p2:
  disc int[0..5] x in any;
  location:
    initial;
    marked;
    edge e2a do x := x + 1;
    edge e2b when x = 3 or x = 5;
    edge e2c when x >= 2;
    edge e2d when x >= 3;
end
supervisor automaton sup:
  alphabet e2a, e2b, e2c, e2d;
  location:
    initial;
    marked;
    edge e2a when p2.x != 5;
    edge e2b when p2.x = 1 or (p2.x = 3 or p2.x = 5);
    edge e2c when 2 <= p2.x and p2.x <= 5;
    edge e2d when not(p2.x = 2 or p2.x = 3);
end
plant invariant e2b needs p2.x >= 3;
plant invariant p2.x = 1 disables e2c;
plant invariant e2d needs p2.x >= 4;
