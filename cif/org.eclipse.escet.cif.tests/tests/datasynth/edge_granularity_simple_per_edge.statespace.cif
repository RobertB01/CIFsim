uncontrollable e;
controllable a;
controllable b;
automaton statespace:
  alphabet e, a, b;
  @state(p1 = "l1", p2 = "l1", p2.x = 0, sup = "*")
  location loc1:
    initial;
    marked;
  @state(p1 = "l1", p2 = "l1", p2.x = 1, sup = "*")
  location loc2:
    initial;
    marked;
    edge e goto loc4;
  @state(p1 = "l1", p2 = "l1", p2.x = 2, sup = "*")
  location loc3:
    initial;
    marked;
    edge e goto loc5;
  @state(p1 = "l2", p2 = "l1", p2.x = 1, sup = "*")
  location loc4:
    edge e goto loc2;
  @state(p1 = "l2", p2 = "l2", p2.x = 2, sup = "*")
  location loc5:
    edge a goto loc6;
  @state(p1 = "l2", p2 = "l1", p2.x = 2, sup = "*")
  location loc6:
    edge e goto loc7;
  @state(p1 = "l1", p2 = "l2", p2.x = 2, sup = "*")
  location loc7:
    edge a goto loc3;
end
