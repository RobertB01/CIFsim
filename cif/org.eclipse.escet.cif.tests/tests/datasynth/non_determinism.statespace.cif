group p:
  controllable c1;
  controllable c2;
  uncontrollable u;
end
automaton statespace:
  alphabet p.c1, p.c2, p.u;
  @state(p = "l0", p.x1 = false, p.x2 = false, sup = "*")
  location loc1:
    initial;
    marked;
    edge p.c2 goto loc2;
  @state(p = "l0", p.x1 = false, p.x2 = true, sup = "*")
  location loc2:
    marked;
    edge p.c2;
    edge p.u goto loc3;
  @state(p = "l2", p.x1 = false, p.x2 = true, sup = "*")
  location loc3:
    edge p.u goto loc2;
end
