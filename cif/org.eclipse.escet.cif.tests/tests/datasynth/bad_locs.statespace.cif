controllable c;
uncontrollable u;
automaton statespace:
  alphabet c, u;
  @state(p = "l0", p.x = 0, sup = "*")
  location loc1:
    initial;
    marked;
    edge c goto loc2;
  @state(p = "l1", p.x = 1, sup = "*")
  location loc2:
    marked;
    edge u goto loc3;
  @state(p = "l2", p.x = 2, sup = "*")
  location loc3:
    marked;
end
