controllable e1;
uncontrollable e2;
automaton statespace:
  alphabet e1, e2;
  @state(p = "L0", p.x = 2, sup = "*")
  location loc1:
    initial;
    marked;
    edge e1 goto loc2;
  @state(p = "L1", p.x = 4, sup = "*")
  location loc2:
    edge e2 goto loc3;
  @state(p = "L0", p.x = 5, sup = "*")
  location loc3:
    marked;
end
