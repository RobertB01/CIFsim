group gA:
  group def A():
    b: gB.B();
  end
end

group gB:
  group def B():
    c: gC.C();
  end
end

group gC:
  automaton def C():
    event e;
    location:
      initial;
      edge ga.a.b.c.e;
  end
end

group ga:
  a: gA.A();
end
