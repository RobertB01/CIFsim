event e;

automaton a:
  disc int[0..10] x in any;
  disc int[0..10] y in any;

  location f:
    edge e goto t2;
    edge e goto p1;

  location t1:
    initial;
    edge e goto f;

  location t2:
    initial true;

  location p1:
    initial x > 1, y < 2;
end
