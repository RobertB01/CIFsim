group grp1:
  automaton aut1:
    event evt1;

    location loc:
      initial;
      edge evt1;
  end

  automaton aut1b:
    location:
      initial;
      edge aut1.evt1;
  end
end

group defs2:
  group def Grp2():
    automaton def Aut2(event param2; location param3):
      location:
        initial param3;
        edge param2;
    end

    aut2: Aut2(grp1.aut1.evt1, grp1.aut1.loc);

    automaton aut2b:
      location:
        initial;
        edge grp1.aut1.evt1;
    end
  end
  grp2: Grp2();
end
