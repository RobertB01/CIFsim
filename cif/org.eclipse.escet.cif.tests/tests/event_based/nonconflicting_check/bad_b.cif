controllable a, b;

plant p:
  location s1:
    initial;
    edge a goto s2;
    edge b goto s3;
  location s2:
    edge a,b goto s4;
  location s3:
    edge b goto s4;
  location s4:
    marked;
end

supervisor s:
  location s1:
    initial;
    edge a goto s2;
    edge b goto s3;
  location s2:
    edge b goto s4;
  location s3:
    edge a goto s4;
  location s4:
    marked;
end
