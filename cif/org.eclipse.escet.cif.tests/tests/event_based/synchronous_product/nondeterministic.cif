controllable a;

plant x:
  location s1:
    initial;
    edge a goto s2;
    edge a goto s3;

  location s2:
    edge a goto s1;

  location s3;
end

plant y:
  location s1:
    initial;
    edge a goto s2;
    edge a goto s3;

  location s2;

  location s3:
    edge a goto s1;
end
