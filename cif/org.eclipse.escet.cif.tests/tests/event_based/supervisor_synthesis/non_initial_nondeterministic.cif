controllable e;

plant p:
  location s1:
    initial;
    marked;
    edge e goto s2;

  location s2:
    edge e goto s3;

  location s3: // (initial) -> e -> e, has two "e" edges.
    edge e;
    edge e;
end

requirement r:
  location:
    initial;
    marked;
    edge e;
end
