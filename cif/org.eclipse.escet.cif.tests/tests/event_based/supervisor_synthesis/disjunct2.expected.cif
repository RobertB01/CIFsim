uncontrollable p;
uncontrollable q;
uncontrollable r;
controllable x;
controllable y;
supervisor automaton sup:
  alphabet p, q, r, x, y;
  location s1:
    initial;
    marked;
    edge q goto s2;
    edge p goto s3;
  location s2:
    edge y;
    edge x;
    edge r goto s5;
    edge p goto s4;
  location s3:
    edge q goto s4;
    edge p goto s1;
  location s4:
    edge y;
    edge x;
    edge r goto s6;
    edge p goto s2;
  location s5:
    edge q goto s7;
    edge p goto s6;
  location s6:
    edge q goto s8;
    edge p goto s5;
  location s7:
    edge y;
    edge x;
    edge r goto s1;
    edge p goto s8;
  location s8:
    edge y;
    edge x;
    edge r goto s3;
    edge p goto s7;
end
