plant p:
  controllable e1, e2;
  location l:
    initial;
    marked;
    edge e1, e2;
end

requirement r1:
  controllable e3;
  location l:
    initial;
    marked;
    edge p.e1, e3;
end

requirement r2:
  controllable e4;
  location l:
    initial;
    marked;
    edge p.e2, e4;
end
