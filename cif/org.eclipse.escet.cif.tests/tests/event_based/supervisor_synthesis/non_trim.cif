plant p:
  controllable u1, u2;
  location l1:
    initial; marked;
    edge u1 goto l2;
  location l2:
    edge u2 goto l3; // not coreachable
  location l3:
    edge u2 goto l2; // not coreachable
end

requirement r:
  location l1:
    initial; marked;
  location l2;       // not reachable
end
