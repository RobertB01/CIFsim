controllable a,b,c, t;

plant G:
  location s0:
    initial;
    edge t goto s1;
  location s1:
    edge a goto s2;
  location s2:
    edge b goto s3;
  location s3:
    edge c goto s4;
  location s4:
    marked;
    edge b goto s1;
end
