automaton A:
  disc list int xs = [1,2,3];
  disc list list int ys = [[1, 23], [4, 5, 6]];

  location X:
    initial;
    edge do xs[1] := 5 goto Y;

  location Y:
    edge do ys[1][0] := 8 goto Z;

  location Z:
    edge do ys[0] := [7], ys[1][1] := 9 goto done;

  location done;
end
