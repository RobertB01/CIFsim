automaton statespace:
  alphabet;
  @state(aut = "*", aut.v = 0)
  location loc1:
    initial;
    edge tau goto loc3;
  @state(aut = "*", aut.v = 2)
  location loc2:
    initial;
    edge tau goto loc4;
  @state(aut = "*", aut.v = 1)
