automaton A:
  disc tuple(bool x,y) xs = (true, false);
  disc tuple(tuple(bool a, b) c,d) ys = ((false, false), (true, true));

  location X:
    initial;
    edge do xs[1] := xs[0] goto Y;

  location Y:
    edge do ys[1][0] := false goto Z;

  location Z;
end
