automaton A:
  disc dict(int: bool) xs = {1:true, 2:false, 3:true};
  disc dict(int:dict(bool:bool)) ys = {0:{true:true, false:false}, 1:{true:true, false:true}};

  location X:
    initial;
    edge do xs[1] := false goto Y;

  location Y:
    edge do ys[1][true] := false goto Z;

  location Z;
end
