event e, f, g;

automaton p:
  location l0:
    initial;
    edge e goto l0;
    edge e goto l1;

  location l1:
    edge f goto l0;
    edge f goto l0;
    edge g;
    edge g;
    edge g goto l0;
    edge g goto l0;
end

automaton q:
  location l0:
    initial;
    edge e goto l0;
    edge e goto l1;

  location l1:
    edge f goto l0;
    edge f goto l0;
    edge g;
end
