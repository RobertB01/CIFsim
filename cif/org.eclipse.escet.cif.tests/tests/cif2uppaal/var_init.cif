// Variables may not be initialized with other variables, so that needs to be
// evaluated away.

event e;

const int c = 5;

plant p:
  disc int x1 = x3 * 2;  // 16
  disc int x2 = c;       // 5
  disc int x3 = x2 + 3;  // 8

  disc int[0..20] v;

  location:
    initial;
    edge e do v := x1;
end
