event e1, e2;

automaton a1:
  alphabet e1, e2;          // Disables e2 globally.

  location:                 // Nameless location.
    initial;
    edge e1;
end

automaton a2:
  location:                 // Nameless location.
    initial;
    edge e1, e2;
end
