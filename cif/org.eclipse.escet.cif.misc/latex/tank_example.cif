group tank:
  cont V = 10.0;
  alg real Qi = controller.n * 5.0;
  alg real Qo = sqrt(V);
  equation V' = Qi - Qo;
end

automaton controller:
  alg int n;

  location closed:
    initial;
    equation n = 0;
    when tank.V <= 2  goto opened;

  location opened:
    equation n = 1;
    when tank.V >= 10 goto closed;
end

string test: "some \n \t \\ \" string"

operator test: 9 div 4


